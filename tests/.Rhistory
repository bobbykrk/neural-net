points <- function(vals, file.name){
fr <- data.frame()
for(i in 1:length(vals)){
pts1.frame <- data.frame(vals[i])
pts1.frame$col <- rep(i,nrow(pts1.frame))
fr <- rbind(fr, pts1.frame)
}
write.table(fr,file.name, row.names=FALSE, col.names=FALSE, sep=",")
return(fr)
}
